<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="Application">
        <xs:complexType>
            <xs:sequence>
				
                <xs:element name="name" minOccurs="1" nillable="false">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>